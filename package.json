{
  "name": "gatsby-typescript-tailwindcss",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop:gatsby": "gatsby develop",
    "develop:type-check": "tsc --watch",
    "dev": "npm-run-all --parallel develop:gatsby develop:type-check",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "gatsby": "^2.24.84",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-remark": "^2.8.44",
    "postcss": "^8.2.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tailwindcss": "^2.0.2",
    "ts-node": "^9.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "typescript": "^4.0.3"
  }
}
